		<jar destfile="${plugin.destination}/OntologyReastrictedNets_1.0.0.jar" basedir="${temp.folder}/OntologyReastrictedNets_1.0.0" filesetmanifest="merge"/>
		<delete dir="${temp.folder}"/>
	</target>

