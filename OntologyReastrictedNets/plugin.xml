<plugin>

   <extension point="org.eclipse.emf.ecore.generated_package">
      <package
            uri="http://k1s.org/epnk/orn"
            class="orn.OrnPackage"
            genModel="model/ORN.genmodel"/>
   </extension>

   <extension point="org.eclipse.emf.ecore.extension_parser">
      <parser
            type="orn"
            class="orn.util.OrnResourceFactoryImpl"/>
   </extension>


<extension
         id="org.k1s.epnk.orn"
         name="ORN"
         point="org.pnml.tools.epnk.pntd">
      <type
            class="orn.impl.ORNTypeImpl"
            description="ORN">
      </type>
   </extension>

